{
  "name": "@yfi/design",
  "version": "0.2.1",
  "description": "Yearn Aesthetic",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.umd.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "babel:watch": "babel ./src -d ./dist/es --watch",
    "build": "rollup -c && babel ./src -d ./dist/es",
    "build-docs": "build-storybook --docs",
    "build-storybook": "build-storybook",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn run build",
    "rollup:watch": "rollup -c -w",
    "start": "npm-run-all -r -p rollup:watch babel:watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "babel src -d dist --extensions \".js,.jsx,.ts,.tsx\" --ignore \"**/*.test.js\" --ignore \"**/*.stories.js\"",
    "storybook:lint": "yarn lint:js && yarn lint:package",
    "storybook:release": "dotenv yarn build && auto shipit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.html": [
      "yarn lint:js --fix"
    ],
    "src/**/*.{js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "*.js": [
      "yarn lint:js --fix"
    ],
    "*.ts?(x)": [
      "yarn lint:js --fix"
    ],
    "package.json": [
      "yarn lint:package"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@rimble/icons)/)"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.6",
    "@d8660091/react-popper": "^1.0.4",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@rimble/icons": "^1.1.0",
    "@styled-system/prop-types": "^5.1.2",
    "@styled-system/theme-get": "^5.1.2",
    "@svgr/rollup": "^4.2.0",
    "@types/pluralize": "^0.0.29",
    "clipboard": "^2.0.4",
    "copy-to-clipboard": "^3.3.1",
    "ethereum-blockies": "^0.1.1",
    "framer-motion": "^3.2.2-rc.1",
    "mixin-deep": "^2.0.1",
    "pluralize": "^8.0.0",
    "polished": "^3.6.4",
    "prismjs": "1.20.0",
    "prop-types": "^15.5.4",
    "qrcode.react": "^0.9.3",
    "react-github-button": "^0.1.11",
    "react-modal": "^3.11.2",
    "react-popper-tooltip": "^2.11.1",
    "set-value": "^3.0.1",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.5",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@semantic-release/npm": "^7.0.10",
    "@storybook/addon-a11y": "^6.1.0",
    "@storybook/addon-actions": "^6.1.0",
    "@storybook/addon-docs": "^6.1.0",
    "@storybook/addon-essentials": "^6.1.0",
    "@storybook/addon-storysource": "^6.1.0",
    "@storybook/eslint-config-storybook": "^2.4.0",
    "@storybook/linter-config": "^2.5.0",
    "@storybook/react": "^6.1.0",
    "@types/fs-extra": "^9.0.1",
    "@types/styled-components": "^5.1.0",
    "auto": "^9.50.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chromatic": "^5.6.1",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^4.2.5",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^10.2.9",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-url": "^2.2.2",
    "seedrandom": "^3.0.5",
    "semantic-release": "^17.3.7",
    "sort-package-json": "^1.44.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  }
}
